{{ form_start(contactForm) }}
<div class="row">
	<div class="col-md-6 col-lg-6">
		{{ form_row(contactForm.firstname) }}
	</div>
	<div class="col-md-6 col-lg-6">
		{{ form_row(contactForm.lastname) }}
	</div>
	<div class="col-md-6 col-lg-6">
		{{ form_row(contactForm.email) }}
	</div>
	<div class="col-md-6 col-lg-6">
		{{ form_row(contactForm.phone) }}
	</div>
	<div class="col-lg-12">
		{{ form_row(contactForm.object) }}
	</div>
	<div class="col-lg-12">
		{{ form_row(contactForm.message) }}
	</div>
	<div class="col-lg-12 mt-3">
		<button type="submit" name="q_submit">
			<span>
				<i class="bx bx-mail-send mr-3"></i>
				{% trans %}
				ENVOYER
				{% endtrans %}
			</span>
		</button>
	</div>
</div>
{{ form_end(contactForm) }}
